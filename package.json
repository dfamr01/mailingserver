{
  "name" : "mailing-server",
  "version" : "0.0.1",
  "private" : true,
  "description" : "Stream me mailing server",
  "scripts" : {
    "start" : "cross-env NODE_ENV=local npm run debug:server",
    "start:test" : "cross-env NODE_ENV=development nodemon --inspect=5656 server/server.js --exec 'npm run lint && node'",
    "debug:server" : "nodemon --inspect=5656 server/server.js",
    "lint" : "cross-env NODE_ENV=development eslint --ext .js --color --format=node_modules/eslint-formatter-pretty server",
    "lint:watch" : "npm run lint --watch",
    "lint:test" : "npm-run-all --parallel lint:watch",
    "test:unit" : "cross-env NODE_ENV=test mocha --time --delay --exit \"server/**/*.spec*.js\" --timeout 0",
    "test" : "npm-run-all --parallel lint test:unit"
  },
